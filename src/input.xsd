<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="input">
        <xs:complexType>
            <xs:sequence>
                <xs:element type="xs:string" name="inputfile"/>
                <xs:element type="xs:integer" name="frequency"/>
                <xs:element type="xs:double" name="delta_t"/>
                <xs:element type="xs:double" name="end_t"/>
                <xs:element type="xs:string" name="outputfile"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
